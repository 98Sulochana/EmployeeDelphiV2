program Employee;

uses
  Vcl.Forms,
  MainU in 'MainU.pas' {MainF},
  DataU in 'DataU.pas' {DataM: TDataModule};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TDataM, DataM);
  Application.CreateForm(TMainF, MainF);
  Application.Run;
end.
